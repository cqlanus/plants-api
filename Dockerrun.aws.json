{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "postgres-data",
      "host": {
        "sourcePath": "/var/lib/postgresql/data"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "plants-api",
      "image": "cqlanus/plants:latest",
      "memory": 128,
      "essential": true,
      "command": ["/usr/local/bin/npm", "run", "dev"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 9001
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "POSTGRES_HOST",
          "value": "plants.chimlwu7ixbj.us-east-2.rds.amazonaws.com"
        },
        {
          "name": "POSTGRES_DB",
          "value": "plants"
        },
        {
          "name": "POSTGRES_USER",
          "value": "clanus"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "password"
        }
      ]
    }
  ]
}
